{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "quotemark": [
      false,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false,
    "no-console": false,
    "member-access": [
      false
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "trailing-comma": [
      false
    ],
    "object-literal-key-quotes": [
      false,
      "as-needed"
    ]
  }
}